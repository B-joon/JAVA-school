package com.test02;

import java.util.Random;

public class RandomCube {

	public static void main(String[] args) {
		/*
		 * 1 ~ 9 사이의 난수로 이루어진
		 * 5 * 5 형태의 숫자들을 출력하고,
		 * 전체 난수의 합,
		 * 전체 난수의 평균,
		 * x의 합을 구하자
		 */
		prnCube();

	}
	
	public static void prnCube() {
		
		Random ran = new Random();
		
		int r = ran.nextInt(10);
		System.out.println(r);
		
		for (int i = 0; i < 10; i++) {
			System.out.println(r);
			if (i % 5 == 0) {
				System.out.println();
			}
		}
		
		
		
		
	}

}
