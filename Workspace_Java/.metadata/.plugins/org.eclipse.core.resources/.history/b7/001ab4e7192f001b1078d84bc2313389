package com.homework;

public class Test {

	public static void main(String[] args) {
		
		for (int i = 1; i < 6; i++) {
			for (int j = 6; j > 0; j--) {
				if (i < j) {
					System.out.print(" ");
				}else {
					System.out.print("*");
				}
				
			}
			System.out.println();
		}

	}
}

// 야구게임 숫자 3개로 10번