package com.test01;

public class MTest {

	public static void main(String[] args) {
		
		//prn01();
		prn02();
		
	}
	
	public static void prn01() {
		
		// 초기값
		int i = 1;
		
		// 조건식
		while (i < 10) {
			System.out.println(i);
			
			i++; // 증감식
		}
		System.out.println("while이 종료된 이후의 i :" + i);
	}
	
	public static void prn02() {
		
		int i = 1;
		
		do {
			System.out.println(i);
			i++;
			
		} while (i < 10);
		System.out.println("while이 끝난 후의 i :" + i);
	}

}
