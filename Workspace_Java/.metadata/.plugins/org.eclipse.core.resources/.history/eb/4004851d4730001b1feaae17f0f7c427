package com.test01;

import org.graalvm.compiler.lir.amd64.vector.AMD64VectorMove.SaveRegistersOp;

public class MTest {

	public static void main(String[] args) {
		
		prn01();
		
	}
	
	public static void prn01() {
		
		// 초기값
		int i = 1;
		
		// 조건식
		while (i < 10) {
			System.out.println(i);
			
			i++; // 증감식
		}
		System.out.println("while이 종료된 이후의 i :" + i);
	}

}
