package com.test01;

public class MethodTest01 {
	
	// main method : 프로그램의 주 진입점
	public static void main(String[] args) {
		

	}
	
	// method :기능, 명령문들의 집합
	// 접근제한자 메모리영역 리턴타입 메소드명 (파라미터) {}
	
	public static void publicMethod() {
		System.out.println("public method 호출!");
	}
	
	// prtected : 상속일 경우 상속된 곳에서, 상속이 아닌 경우 같은 패키지 내에서 (#)
	protected static void protectedMethod() {
		System.out.println("prtected method 호출!");
	}
	
	// (default) : 같은 패키지 내에서만 (~)
	static void defaultMethod() {
		System.out.println("default method 호출!");
	}
}
