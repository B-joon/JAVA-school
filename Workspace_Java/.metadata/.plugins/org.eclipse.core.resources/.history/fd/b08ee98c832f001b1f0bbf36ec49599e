package com.test01;

public class MTest {

	public static void main(String[] args) {
		prn01();

	}
	public static void prn01() {
		int i = 10;
		int j = 20;
		
		// if : 만약 ~ 라면
		if (i < j) { // 만약 조건이 참이라면
			System.out.println("i는 j보다 작습니다.");
		}
		if (i > j) {
			System.out.println("i는 j보다 큽니다.");
		} else {
			System.out.println("i는 j보다 크지 않습니다.");
		}
		
		if (i == 2) {
			System.out.println("i는 2입니다.");
		} else if (i == 5) {
			System.out.println("i 는 5입니다.");
		} else if (i == 10) {
			System.out.println("i 는 10 입니다.");
		} else if (i == 20) {
			System.out.println("i는 20 입니다.");
		}
	}

}
