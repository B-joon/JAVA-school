package com.test01;

public class MTest {

	public static void main(String[] args) {
		
		prn01();

	}
	public static void prn01() {
		int i = 2;
		
		switch (i) {
		case 1:
			System.out.println("1입니다");

		case 2:
			System.out.println("2입니다.");
			
		case 3:
			System.out.println("3입니다.");
		}
	}

}
